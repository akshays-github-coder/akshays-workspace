<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DemoServiceService WSDL service -->

  <service name="DemoService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://com"/>
      <parameter name="wsdlServiceElement" value="DemoServiceService"/>
      <parameter name="schemaQualified" value="http://com"/>
      <parameter name="wsdlServicePort" value="DemoService"/>
      <parameter name="className" value="com.DemoServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="DemoService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="square" qname="operNS:square" xmlns:operNS="http://com" returnQName="retNS:squareReturn" xmlns:retNS="http://com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:n" xmlns:pns="http://com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="square"/>

  </service>
</deployment>
